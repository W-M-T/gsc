// Internally, the type gets generalized to âˆ€a.a â†’ a
id(x) :: a -> a {
    return x;
}

// You should now be able to write the polymorphic list reversal
reverse(list) :: [a] -> [a] {
    [a] res = [];
    [a] iter = [];

    for(iter=list; iter!=[]; iter=iter.tl) {
        res = iter.hd : res;
    }
    return res;
}


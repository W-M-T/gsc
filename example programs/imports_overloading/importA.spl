infixl 7 ++ (a, b) :: Int Int -> Int {
    print('+');
    print('+');
    print('I');

    return a + b + 3;
}

infixl 7 ++ (a, b) :: Bool Bool -> Bool {
    print('+');
    print('+');
    print('B');

    return True;
}

infixl 7 -- (a, b) :: Int Int -> Int {
    print('-');
    print('-');
    print('I');

    return a + b - 1;
}

infixl 7 -- (a, b) :: Bool Bool -> Bool {
    print('-');
    print('-');
    print('B');

    return False;
}
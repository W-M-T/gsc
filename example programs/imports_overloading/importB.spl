infixl 7 ** (a, b) :: Int Bool -> Int {
    print('*');
    print('*');
    print('I');

    return a;
}

infixl 7 ** (a, b) :: Bool Int -> Bool {
    print('*');
    print('*');
    print('B');

    return a;
}

infixl 7 <* (a, b) :: Int Bool -> Int {
    print('<');
    print('*');
    print('I');

    return a;
}

infixl 7 <* (a, b) :: Bool Int -> Bool {
    print('<');
    print('*');
    print('B');

    return a;
}